{
    "name": "elegantweb/laravel-datatables",
    "description": "DataTables library for the Laravel framework.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sina Sharifzade",
            "email": "sharifzadesina@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Elegant\\DataTables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elegant\\DataTables\\Tests\\": "tests/",
            "Elegant\\DataTables\\Tests\\Database\\Factories\\": "tests/database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elegant\\DataTables\\DataTablesServiceProvider"
            ],
            "aliases": {
                "DataTables": "Elegant\\DataTables\\Support\\Facades\\DataTables"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
