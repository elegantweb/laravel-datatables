{
    "name": "elegantweb/laravel-datatables",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sina Sharifzade",
            "email": "sharifzadesina@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "laravel/framework": "5.8.*|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Elegant\\DataTables\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elegant\\DataTables\\DataTablesServiceProvider"
            ],
            "aliases": {
                "DataTables": "Elegant\\DataTables\\Support\\Facades\\DataTables"
            }
        }
    }
}
